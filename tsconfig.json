{
  "compilerOptions": {
    "jsx": "react",
    "target": "es5",
    "outDir": "dist",
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "module": "ESNext",
    "allowSyntheticDefaultImports": true,
    "moduleResolution": "Node",
    "declaration": true,
    "declarationDir": "types",
    "sourceMap": true,
    "emitDeclarationOnly": true,
    "esModuleInterop": true,
    "resolveJsonModule": true
  }
}
